<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="ebay Automation Script " parallel="true">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="66.0"/>
    <parameter name="url" value="https://reg.ebay.com/reg/PartialReg?ru=https%3A%2F%2Fwww.ebay.com%2F"/>

    <test name = "ebay Home">
        <classes>
            <class name="SearchPage.Registration"/>
        </classes>
    </test>

</suite>