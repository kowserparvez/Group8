<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="FaceBook Automation Test" parallel="methods">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="Windows"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.costco.com/"/>

    <test name = "Costco Home">
        <classes>
            <class name="home.page.TestSearchItems"/>
            <!--<class name="facebook.activities.TestToFaceBookActivities"/>-->
        </classes>
    </test>
</suite>
